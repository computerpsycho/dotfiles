[disk]
command=$SCRIPT_DIR/disk
LABEL=disk (free): 
interval=30

[memory]
label=mem: 
interval=10
command=$SCRIPT_DIR/memory

[cpu_usage]
label=cpu: 
interval=10
command=$SCRIPT_DIR/cpu_usage

[iface]
label=ipv4:
command=$SCRIPT_DIR/iface
interval=10

[batterybar]
command=$SCRIPT_DIR/batterybar
label=bat: 
interval=5
markup=pango
min_width=bat: ■■■■■

[time]
interval=1
STRFTIME_FORMAT=%Y-%m-%d %R
TZONES=$DEFAULT_TZ,Brazil/East,Australia/Brisbane,Asia/Calcutta
TZ_LABELS=,Brazil,AU,Hyderabad
command=$SCRIPT_DIR/time
